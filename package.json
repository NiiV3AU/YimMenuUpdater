{
  "name": "ymu-website",
  "version": "1.0.0",
  "description": "The official website for the YimMenuUpdater (YMU) project, built with Eleventy.",
  "scripts": {
    "start": "npx @11ty/eleventy --serve",
    "build": "cross-env NODE_ENV=production npx @11ty/eleventy && npm run minify-js",
    "minify-js": "terser _site/assets/js/script.js --config-file terser.config.json --output _site/assets/js/script.js"
  },
  "keywords": [
    "eleventy",
    "11ty",
    "static-site",
    "yimmenu",
    "ymu",
    "yimmenuupdater"
  ],
  "author": "NiiV3AU",
  "license": "CC-BY-NC-SA-4.0",
  "devDependencies": {
    "@11ty/eleventy": "3.1.2",
    "cross-env": "^10.1.0",
    "html-minifier-next": "4.4.0",
    "terser": "5.44.1"
  }
}
